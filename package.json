{
  "name": "skylark-chartjs",
  "description": "A version of chartjs that ported to running on skylarkjs",
  "version": "0.9.2",
  "keywords": [
    "skylark",
    "chart.js"
  ],
  "homepage": "https://github.com/skylark-integration/skylark-chartjs/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "sbundle build ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-integration/skylark-chartjs.git"
  },
  "bugs": {
    "url": "https://github.com/skylark-integration/skylark-chartjs/issues"
  },
  "license": "MIT",
  "dependencies": {
    "skylark-langx": "git+https://github.com/skylark-langx/skylark-langx.git",
    "skylark-moment": "git+https://github.com/skylark-integration/skylark-moment.git"
  },
  "skylarkjs": {
    "build": {
      "prepare": {
        "texttojs": [
          "css",
          "html"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      }
    }
  },
  "engines": {
    "node": ">=0.10.1"
  }
}
