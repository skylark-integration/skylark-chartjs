{
  "name": "skylark-chartjs",
  "description": "A version of chartjs that ported to running on skylarkjs",
  "version": "0.9.2",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://github.com/skylarkui/skylark-chartjs/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "sbundle build ."
  },
  "style": "dist/css/bootstrap.css",
  "less": "less/bootstrap.less",
  "main": "./dist/js/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/skylarkui/skylark-chartjs.git"
  },
  "bugs": {
    "url": "https://github.com/skylarkui/skylark-chartjs/issues"
  },
  "license": "MIT",
  "dependencies": {
    "skylark-bootstrap3": "git+https://github.com/skylarkui/skylark-bootstrap3.git#master",
    "skylark-langx": "git+https://github.com/skylarklangx/skylark-langx.git#master",
    "skylark-utils-dom": "git+https://github.com/skylarkutils/skylark-utils-dom.git#master"
  },
  "skylarkjs" : {
    "build" : {
      "prepare" : {
        "texttojs" : ["css","html"],
        "jsxtojs" : false,
        "es6toamd" : false,
        "cjstoamd" : false
      }
    }
  } ,  
  "engines": {
    "node": ">=0.10.1"
  }
}
